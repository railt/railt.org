{
    "case_insensitive": false,
    "contains": [
        {
            "className": "comment",
            "begin": "#",
            "end": "$"
        },
        {
            "className": "addition",
            "begin": "[→✓✔]",
            "end": ".+\\n",
            "excludeBegin": true
        },
        {
            "className": "deletion",
            "begin": "[✕✖]",
            "end": ".+\\n",
            "excludeBegin": true
        },
        {
            "className": "keyword",
            "begin": "→"
        }
    ]
}
