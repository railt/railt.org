{
    "description": "Website of The Railt GraphQL Framework",
    "keywords": [
        "framework",
        "railt.org",
        "graphql"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "barryvdh/laravel-debugbar": "~3.1",
        "barryvdh/laravel-ide-helper": "~2.4",
        "beberlei/DoctrineExtensions": "~1.0",
        "doctrine/dbal": "~2.6",
        "erusev/parsedown": "~1.6",
        "fideloper/proxy": "~4.0",
        "graham-campbell/github": "~6.0",
        "itsgoingd/clockwork": "~2.0",
        "laravel-doctrine/orm": "~1.4",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "lcobucci/jwt": "^3.2",
        "php-http/guzzle6-adapter": "~1.1",
        "predis/predis": "~1.1",
        "railt/laravel-provider": "dev-master@dev",
        "railt/railt": "dev-master@dev",
        "sebdesign/laravel-sri": "^1.0",
        "serafim/hydrogen": "dev-master@dev",
        "serafim/laravel-annotations": "~1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
